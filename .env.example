# =================================================================
# Oracle APEX + ORDS Docker Environment Variables
# Copy this file to .env and modify the values
# =================================================================

# Oracle Database Configuration
ORACLE_PWD=YourSecurePassword123

# Database Ports
DB_PORT=1521
DB_EM_PORT=5500

# ORDS Configuration
ORDS_PORT=8181
ORDS_HTTPS_PORT=8443

# APEX Users Passwords
APEX_PUBLIC_USER=ApexPublic123
APEX_LISTENER=ApexListener123
APEX_REST=ApexRest123

# =================================================================
# Coolify Specific Settings
# =================================================================
# These are used by Coolify for reverse proxy configuration

# The domain where APEX will be accessible
APEX_DOMAIN=apex.yourdomain.com

# Enable HTTPS (handled by Coolify's Traefik)
ENABLE_HTTPS=true
